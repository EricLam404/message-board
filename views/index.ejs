<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% messages.forEach(function(message) { %>
        <li>
          <%= message.text %>
          <br>
          <span>by <%= message.user %></span>
          <span>on <%= message.added %></span>
        </li>
      <% }); %>
    </ul>
  </body>
</html>
